home = "springfield"

for i in range(10)
  if home == "France":
    print("Hello, France")
  
  if home == "Japan":
    print("Hello, Japan")
    
  if home == "Taiwan":
    print("Hello, Taiwan")
  else:
    print("I will not leave a bee hive in Principle Skinner's office")
